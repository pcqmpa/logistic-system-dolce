{
  "name": "logistic-system-dolce",
  "version": "0.0.2",
  "description": "Simple system to manage all orders based on the zones",
  "main": "./src/server/index.js",
  "scripts": {
    "dev": "concurrently --kill-others -p \"[{name}]\" -n \"BUILD,API,SERVER\" -c \"yellow.bold,green.bold,blue.bold\" \"yarn run dev:build\" \"yarn run dev:api\" \"yarn run dev:server\"",
    "dev:mock": "MOCK_SERVER=1 concurrently --kill-others -p \"[{name}]\" -n \"BUILD,API,SERVER\" -c \"yellow.bold,green.bold,blue.bold\" \"yarn run dev:build\" \"yarn run dev:mock:api\" \"yarn run dev:server\"",
    "dev:build": "NODE_ENV=development && babel-node ./src/server/dev-server.js",
    "dev:server": "NODE_ENV=development && nodemon --exec babel-node ./src/server/index.js --watch ./src ./build",
    "dev:api": "NODE_ENV=development && nodemon --exec babel-node ./src/server/api-server.js --watch src/server",
    "dev:mock:api": "NODE_ENV=development && nodemon --exec babel-node ./src/server/mock-api-server.js --watch src/server",
    "build": "NODE_ENV=production webpack --verbose --colors --display-error-details --config ./webpack/production/webpack.config.js",
    "prod": "yarn run prod:start",
    "prod:mock": "concurrently --kill-others -p \"[{name}]\" -n \"API,SERVER\" -c \"green.bold,blue.bold\" \"yarn run prod:mock:api\" \"yarn run prod:server\"",
    "prod:start": "concurrently --kill-others -p \"[{name}]\" -n \"API,SERVER\" -c \"green.bold,blue.bold\" \"yarn run prod:api\" \"yarn run prod:server\"",
    "prod:server": "NODE_ENV=production && nodemon --exec babel-node ./src/server/index.js --watch ./src ./build",
    "prod:api": "NODE_ENV=production && nodemon --exec babel-node ./src/server/api-server.js --watch src/server",
    "prod:mock:api": "NODE_ENV=production && nodemon --exec babel-node ./src/server/mock-api-server.js --watch src/server",
    "lint": "eslint ./src/ -f table -c .eslintrc --ignore-pattern '!node_modules/*'",
    "lint:watch": "nodemon --exec yarn run lint --watch src/",
    "stylelint": "stylelint \"src/client/styles/**/*.scss\"",
    "esdoc": "esdoc",
    "test": "jest tests",
    "test:watch": "jest tests --watchAll"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mateoq/logistic-system-dolce.git"
  },
  "author": "Dolce SAS",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Mateoq/logistic-system-dolce/issues"
  },
  "homepage": "https://github.com/Mateoq/logistic-system-dolce#readme",
  "devDependencies": {
    "eslint": "^4.3.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.1.0",
    "nodemon": "^1.11.0",
    "redux-devtools-dock-monitor": "^1.1.2",
    "redux-devtools-log-monitor": "^1.3.0",
    "webpack-dev-middleware": "^1.11.0",
    "webpack-hot-middleware": "^2.18.2"
  },
  "dependencies": {
    "autoprefixer": "^7.1.2",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "blaze": "^3.2.2",
    "body-parser": "^1.17.2",
    "bourbon": "^4.3.4",
    "clean-webpack-plugin": "^0.1.16",
    "compression": "^1.7.0",
    "concurrently": "^3.5.0",
    "connect-mongo": "^1.3.2",
    "cors": "^2.8.4",
    "css-loader": "^0.28.4",
    "enzyme": "^2.9.1",
    "esdoc": "^0.5.2",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-class-property": "^1.0.6",
    "express": "^4.15.3",
    "express-session": "^1.15.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "history": "^4.6.3",
    "http-proxy": "^1.16.2",
    "jest": "^20.0.4",
    "jsonwebtoken": "^7.4.1",
    "mdi": "^2.0.46",
    "multer": "^1.3.0",
    "node-emoji": "^1.8.1",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.6",
    "pretty-error": "^2.1.1",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-addons-css-transition-group": "^15.6.0",
    "react-dom": "^15.6.1",
    "react-hot-loader": "next",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.2",
    "react-router-dom": "^4.1.2",
    "react-router-redux": "^5.0.0-alpha.6",
    "react-test-renderer": "^15.6.1",
    "redux": "^3.7.2",
    "redux-create-reducer": "^1.1.1",
    "redux-devtools": "^3.4.0",
    "redux-observable": "^0.14.1",
    "request": "^2.79.0",
    "rxjs": "^5.4.2",
    "sass-loader": "^6.0.6",
    "serialize-javascript": "^1.4.0",
    "shortid": "^2.2.6",
    "source-map-support": "^0.4.15",
    "split-array": "^1.0.1",
    "style-loader": "^0.18.2",
    "stylelint": "^8.0.0",
    "stylelint-config-standard": "^17.0.0",
    "stylelint-scss": "^1.5.1",
    "stylelint-webpack-plugin": "^0.9.0",
    "url-loader": "^0.5.9",
    "webpack": "^3.4.0",
    "webpack-config": "^7.0.0",
    "webpack-isomorphic-tools": "^3.0.3"
  }
}
