{
  "name": "logistic-system-dolce",
  "version": "0.0.2",
  "description": "Simple system to manage all orders based on the zones",
  "main": "./src/server/index.js",
  "scripts": {
    "dev": "concurrently --kill-others -p \"[{name}]\" -n \"BUILD,API,SERVER\" -c \"yellow.bold,green.bold,blue.bold\" \"yarn run dev:build\" \"yarn run dev:api\" \"yarn run dev:server\"",
    "dev:build": "set NODE_ENV=development && babel-node ./src/server/dev-server.js",
    "dev:server": "set NODE_ENV=development && nodemon --exec babel-node ./src/server/index.js",
    "dev:api": "set NODE_ENV=development && nodemon --exec babel-node ./src/server/api-server.js",
    "build": "NODE_ENV=production webpack --verbose --colors --display-error-details --config ./webpack/production/webpack.config.js",
    "prod": "npm run prod:start",
    "prod:start": "concurrently --kill-others -p \"[{name}]\" -n \"API,SERVER\" -c \"green.bold,blue.bold\" \"npm run prod:api\" \"npm run prod:server\"",
    "prod:server": "NODE_ENV=production babel-node ./src/server/index.js",
    "prod:api": "NODE_ENV=production babel-node ./src/server/api-server.js",
    "lint": "eslint ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mateoq/logistic-system-dolce.git"
  },
  "author": "Dolce SAS",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Mateoq/logistic-system-dolce/issues"
  },
  "homepage": "https://github.com/Mateoq/logistic-system-dolce#readme",
  "devDependencies": {
    "babel-plugin-react-intl": "^2.2.0",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-preset-airbnb": "^2.1.1",
    "eslint": "^3.11.1",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.8.0",
    "nodemon": "^1.11.0",
    "react-hot-loader": "^1.3.1",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "redbox-react": "^1.3.3",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.1.1",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-hot-middleware": "^2.13.2"
  },
